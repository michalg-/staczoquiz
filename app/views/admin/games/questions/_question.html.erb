<%= content_tag :tr, id: dom_id(question), is: 'turbo-frame' do %>
  <td class="border-b border-slate-100 dark:border-slate-700 p-4 pl-8 text-slate-500 dark:text-slate-400"><%= question.id %></td>
  <td class="border-b border-slate-100 dark:border-slate-700 p-4 pl-8 text-slate-500 dark:text-slate-400"><%= question.text %></td>
  <td>
    <%= link_to 'Edit', edit_admin_game_question_path(question.game, question), data: { turbo_stream: true } %>
    <%= link_to 'Delete', admin_game_question_path(question.game, question), data: { turbo_method: :delete } %>
  </td>
<% end %>
